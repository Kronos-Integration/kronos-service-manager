{
  "name": "kronos-service-manager",
  "version": "0.0.0-semantic-release",
  "description": "kronos service manger",
  "main": "dist/module.js",
  "module": "src/ServiceManager.js",
  "scripts": {
    "cover": "nyc --temp-directory build/nyc npm test",
    "test": "mocha tests/*-test.js",
    "semantic-release": "semantic-release",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "pretest": "rollup -c",
    "posttest": "markdown-doctest",
    "precover": "rollup -c",
    "prepare": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kronos-Integration/kronos-service-manager.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-service-manager/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-service-manager#readme",
  "dependencies": {
    "kronos-step": "^5.6.4",
    "kronos-service": "^4.13.3",
    "model-attributes": "^3.0.10",
    "registry-mixin": "^2.1.16"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "kronos-flow": "^2.5.39",
    "jsdoc-to-markdown": "^3.0.3",
    "markdown-doctest": "^0.9.1",
    "mocha": "^4.0.1",
    "nyc": "^11.4.1",
    "rollup": "^0.53.0",
    "semantic-release": "^11.1.0"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=8.9.3"
  },
  "keywords": [],
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/Kronos-Tools/npm-package-template.git"
    }
  },
  "xo": {
    "space": true
  }
}
